[
  {
    "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
    "name": "Google Drive",
    "id": "io.camunda.connectors.GoogleDrive.v1",
    "description": "A Google Drive Connector - Create a folder or a file from template.",
    "category": {
      "id": "connectors",
      "name": "Connectors"
    },
    "appliesTo": [
      "bpmn:ServiceTask"
    ],
    "groups": [
      {
        "id": "authentication",
        "label": "Authentication"
      },
      {
        "id": "create",
        "label": "Create"
      },
      {
        "id": "output",
        "label": "Output Mapping"
      }
    ],
    "properties": [
      {
        "type": "Hidden",
        "value": "io.camunda:google-drive:1",
        "binding": {
          "type": "zeebe:taskDefinition:type"
        }
      },
      {
        "label": "Google Drive Bearer Token",
        "description": "Provide a Google API Bearer token that is authorized to interact with desired Drive. Please use secrets.* to ensure security.",
        "group": "authentication",
        "type": "String",
        "binding": {
          "type": "zeebe:input",
          "name": "token"
        },
        "constraints": {
          "notEmpty": true
        }
      },
      {
        "id": "resourceType",
        "label": "Resource Type",
        "group": "create",
        "description": "Specify resource type you wish to create.",
        "type": "Dropdown",
        "value": "folder",
        "choices": [
          {
            "name": "Folder",
            "value": "folder"
          },
          {
            "name": "File from template",
            "value": "file"
          }
        ],
        "binding": {
          "type": "zeebe:taskHeader",
          "name": "resource.type",
          "key": "resource.type"
        }
      },
      {
        "label": "Resource name",
        "description": "Specify a desired name for your folder or file",
        "group": "create",
        "type": "String",
        "feel": "optional",
        "binding": {
          "type": "zeebe:input",
          "name": "resource.name"
        },
        "constraints": {
          "notEmpty": true,
          "maxLength": 255
        }
      },
      {
        "label": "Template ID",
        "description": "Specify a Google Docs template ID.",
        "group": "create",
        "type": "String",
        "feel": "optional",
        "binding": {
          "type": "zeebe:input",
          "name": "resource.template.id"
        },
        "constraints": {
          "notEmpty": true
        },
        "condition": {
          "property": "resourceType",
          "equals": "file"
        }
      },
      {
        "label": "Parent folder ID",
        "description": "Specify a desired name for your folder or file.",
        "group": "create",
        "type": "String",
        "feel": "optional",
        "binding": {
          "type": "zeebe:input",
          "name": "resource.parent"
        }
      },
      {
        "label": "Template variables",
        "description": "Use this field to pass required template variables.",
        "group": "create",
        "type": "Text",
        "feel": "optional",
        "binding": {
          "type": "zeebe:input",
          "name": "resource.template.variables"
        },
        "condition": {
          "property": "resourceType",
          "equals": "file"
        }
      },
      {
        "label": "Additional Google Drive API properties",
        "description": "Use this field to pass additional Google Drive parameters as JSON.",
        "group": "create",
        "type": "Text",
        "feel": "optional",
        "binding": {
          "type": "zeebe:input",
          "name": "resource.additionalGoogleDriveProperties"
        }
      },
      {
        "label": "Result Variable",
        "description": "Name of variable to store the response in.",
        "group": "output",
        "type": "String",
        "binding": {
          "type": "zeebe:taskHeader",
          "key": "resultVariable"
        }
      },
      {
        "label": "Result Expression",
        "description": "Expression to map the response into process variables",
        "group": "output",
        "type": "Text",
        "feel": "required",
        "binding": {
          "type": "zeebe:taskHeader",
          "key": "resultExpression"
        }
      }
    ],
    "icon": {
      "contents": "data:image/png;base64,"
    }
  }
]